<h1>My Planner</h1>

<ul>
    {{#each plans}}
        <li>
            <p> <!-- whatever that is written in the p tag will be in the same line -->
            
                {{this.id}}. {{this.plan}}

                <button data-planid="{{this.id}}" class="delplan">DELETE PLAN!</button>

            </p>
        </li>
    {{/each}}
</ul>